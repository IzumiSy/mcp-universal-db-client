{
  "compilerOptions": {
    "target": "ES2020",
    "module": "preserve",
    "moduleResolution": "bundler",
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"],
}
